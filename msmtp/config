account andres.acienfuegos@gmail.com
host smtp.gmail.com
port 587
from andres.acienfuegos@gmail.com
user andres.acienfuegos@gmail.com
passwordeval "pass andres.acienfuegos@gmail.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/aacienfuegos/.config/msmtp/msmtp.log

